@model IEnumerable<Proyeto.Models.UsuarioAutorModel>

@{
    ViewData["Title"] = "Usuario";
    Layout = "~/Views/Shared/_Layout_Admin.cshtml";
}


<link rel="stylesheet" href='~/css/StyleSheet1.css' />
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" />
<link rel="stylesheet" href='~/css/footerFelcidades.css' />

<p>
    <a asp-action="Crear">Crear Usuario</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdAutor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApePaterno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ApeMaterno)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Matricula)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumEmpleado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TipoCuenta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumTelefono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FechaNaci)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CuerpoAcademico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AreaEstudios)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NivelEstudio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NombreUsuario)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Correo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdAutor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApePaterno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApeMaterno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Matricula)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumEmpleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TipoCuenta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumTelefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FechaNaci)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CuerpoAcademico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AreaEstudios)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NivelEstudio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NombreUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Correo)
            </td>
            <td>
                @Html.ActionLink("Editar", "Editar", new { id=item.IdAutor }) |
                    @Html.ActionLink("Detalle", "Detalle", new { id=item.IdAutor }) |
                    @Html.ActionLink("Eliminar", "Eliminar", new { id=item.IdAutor })
            </td>
        </tr>
}
    </tbody>
</table>
